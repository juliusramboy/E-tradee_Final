<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>


<AnchorPane prefHeight="155.0" prefWidth="343.0" styleClass="tableview" stylesheets="@design/design.css" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.etradee.Refund">
   <children>
      <Label layoutX="95.0" layoutY="35.0" styleClass="fonts" text="Confirm refund?" />
      <HBox layoutX="57.0" layoutY="78.0" spacing="50.0">
         <children>
            <Button fx:id="refund_yes" mnemonicParsing="false" prefHeight="40.0" prefWidth="90.0" style="-fx-background-color: #D9D9D9; -fx-background-radius: 10px;" text="Yes">
               <styleClass>
                  <String fx:value="btnhover" />
                  <String fx:value="fonts" />
               </styleClass>
            </Button>
            <Button fx:id="refund_no" mnemonicParsing="false" prefHeight="40.0" prefWidth="90.0" style="-fx-background-color: #D9D9D9; -fx-background-radius: 10px;" text="No">
               <styleClass>
                  <String fx:value="btnhover" />
                  <String fx:value="fonts" />
               </styleClass>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
